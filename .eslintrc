{
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": [
    "airbnb/base"
  ],
  "rules": {
    "comma-dangle": [
      "error",
      "never"
    ],
    "import/no-named-as-default": 0,
    "import/no-named-as-default-member": 0,
    "import/extensions": 0,
    "no-useless-escape": 0,
    "import/prefer-default-export": 0,
    "no-underscore-dangle": 0,
    "no-return-await": 0,
    "consistent-return": 0,
    "no-unused-vars": 0,
    "func-names": 0,
    "import/no-extraneous-dependencies": 0
  }
}
